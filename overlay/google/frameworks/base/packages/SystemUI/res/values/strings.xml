<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- Screenshot editing default activity.  Must handle ACTION_EDIT image/png intents.
         Blank sends the user to the Chooser first.
         This name is in the ComponentName flattened format (package/class)  -->
    <string name="config_screenshotEditor">com.google.android.markup/com.google.android.markup.AnnotateActivity</string>

    <!-- Launcher package name for overlaying icons. -->
    <string name="launcher_overlayable_package">com.google.android.apps.nexuslauncher</string>

    <!-- ThemePicker package name for overlaying icons. -->
    <string name="themepicker_overlayable_package">com.google.android.apps.wallpaper</string>
</resources>